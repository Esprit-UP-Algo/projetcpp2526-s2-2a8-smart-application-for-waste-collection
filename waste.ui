/* üìã TABLE PRINCIPALE */
QTableWidget, QTableView {
    background-color: #FFFFFF;
    alternate-background-color: #F5F8FA;
    gridline-color: #D4E1E9;
    border: 2px solid #2C5F7C;
    border-radius: 12px;
    selection-background-color: #3A6EA5;
    selection-color: white;
    font-size: 13px;
    font-weight: 500;
}

/* üìå EN-T√äTE (HEADER) */
QHeaderView::section {
    background-color: qlineargradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #2C5F7C,
        stop:0.5 #3A6EA5,
        stop:1 #1B3A57
    );
    color: white;
    padding: 14px 18px;
    border: none;
    border-right: 1px solid #1B3A57;
    font-weight: 600;
    font-size: 14px;
    text-align: left;
}

QHeaderView::section:hover {
    background-color: qlineargradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #3A6EA5,
        stop:0.5 #4A86C5,
        stop:1 #2C5F7C
    );
}

QHeaderView::section:first {
    border-top-left-radius: 10px;
}

QHeaderView::section:last {
    border-top-right-radius: 10px;
    border-right: none;
}

QHeaderView::section:pressed {
    background-color: qlineargradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #1B3A57,
        stop:1 #2C5F7C
    );
}

/* üìä CELLULES */
QTableWidget::item, QTableView::item {
    padding: 12px 18px;
    border-bottom: 1px solid #E8EFF4;
    color: #1B3A57;
}

QTableWidget::item:selected, QTableView::item:selected {
    background-color: qlineargradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #3A6EA5,
        stop:1 #2C5F7C
    );
    color: white;
    font-weight: 600;
}

QTableWidget::item:hover, QTableView::item:hover {
    background-color: #E3EDF5;
}

/* üìú SCROLLBAR VERTICALE */
QScrollBar:vertical {
    background: #F5F8FA;
    width: 14px;
    border-radius: 7px;
    margin: 0px;
}

QScrollBar::handle:vertical {
    background-color: qlineargradient(
        x1:0, y1:0,
        x2:1, y2:0,
        stop:0 #2C5F7C,
        stop:1 #3A6EA5
    );
    border-radius: 7px;
    min-height: 30px;
}

QScrollBar::handle:vertical:hover {
    background-color: qlineargradient(
        x1:0, y1:0,
        x2:1, y2:0,
        stop:0 #3A6EA5,
        stop:1 #4A86C5
    );
}

QScrollBar::handle:vertical:pressed {
    background-color: #1B3A57;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0px;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
    background: none;
}

/* üìú SCROLLBAR HORIZONTALE */
QScrollBar:horizontal {
    background: #F5F8FA;
    height: 14px;
    border-radius: 7px;
    margin: 0px;
}

QScrollBar::handle:horizontal {
    background-color: qlineargradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #2C5F7C,
        stop:1 #3A6EA5
    );
    border-radius: 7px;
    min-width: 30px;
}

QScrollBar::handle:horizontal:hover {
    background-color: qlineargradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #3A6EA5,
        stop:1 #4A86C5
    );
}

QScrollBar::handle:horizontal:pressed {
    background-color: #1B3A57;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    width: 0px;
}

QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}

/* üîç COIN (intersection scrollbars) */
QTableWidget::corner, QTableView::corner {
    background-color: qlineargradient(
        x1:0, y1:0,
        x2:0, y2:1,
        stop:0 #2C5F7C,
        stop:1 #1B3A57
    );
    border: none;
}

/* üéØ LIGNE ALTERN√âE (optionnel) */
QTableWidget::item:alternate, QTableView::item:alternate {
    background-color: #F5F8FA;
